/*
 *  Date: June 2017
 *  Description: A Pelican theme. Based on HTML5.
 *  Website URI: http://hansliu.com
 */

$padding: 0.6em;
$bodysize: 16px;
$h1size: $bodysize * 2;
$h2size: $h1size * 4/5;
$h3size: $h2size * 9/10;
$h4size: $h3size * 9/10;
$h5size: $h4size * 9/10;
$h6size: $h5size * 9/10;
$iframe16-9: 56.25%;
$iframe4-3: 75%;
$youtubesize: $iframe16-9;
$sidebarwidth: 250px;
$font_sans: "Microsoft YaHei", "Open Sans", sans-serif;
$font_serif: "Microsoft YaHei", Overlock, serif;
$font_code: Menlo, Consolas, monospace;

@import url("pygment.css");
@import url('https://fonts.googleapis.com/css?family=Open+Sans:400,700');
@import url('https://fonts.googleapis.com/css?family=Overlock:400,700');

@mixin headings {
  font-family: $font_serif;
  font-weight: bold;
}

h1 {
  @include headings;
  font-size: $h1size;
  color: #5d686f;
  line-height: $h1size;
}

h2 {
  @include headings;
  font-size: $h2size;
  color: darkblue;
  line-height: $h2size;
}

h3 {
  @include headings;
  font-size: $h3size;
  color: darkblue;
  line-height: $h3size;
}

h4 {
  @include headings;
  font-size: $h4size;
  color: #444;
  line-height: $h4size;
}

h5 {
  @include headings;
  font-size: $h5size;
  color: #555;
  line-height: $h5size;
}

h6 {
  @include headings;
  font-size: $h6size;
  color: #666;
  line-height: $h6size;
}

@media only screen and (max-width:767px) {
  h1 {
    font-size: $h1size * 4/5;
  }
  h2 {
    font-size: $h2size * 4/5;
  }
  h3 {
    font-size: $h3size * 4/5;
  }
  h4 {
    font-size: $h4size * 4/5;
  }
  h5 {
    font-size: $h5size * 4/5;
  }
  h6 {
    font-size: $h6size * 4/5;
  }
}

@media only screen and (min-width:768px) and (max-width:1023px) {
  h1 {
    font-size: $h1size * 9/10;
  }
  h2 {
    font-size: $h2size * 9/10;
  }
  h3 {
    font-size: $h3size * 9/10;
  }
  h4 {
    font-size: $h4size * 9/10;
  }
  h5 {
    font-size: $h5size * 9/10;
  }
  h6 {
    font-size: $h6size * 9/10;
  }
}

pre {
  background-color: #eeeeee;
  padding: 10px;
  margin: 10px;
  font-size: 13px;
  line-height: 20px;
  word-break: break-all;
  word-wrap: break-word;
  white-space: pre-wrap;
  overflow: auto;
}

code, pre>code {
  font-family: $font_code;
}

blockquote {
  margin: 20px;
  font-style: normal;
  border: 0;
  p {
    font-size: 15px;
    margin-bottom: 1em;
    font-weight: normal;
  }
}

strong, b {
  font-weight: bold;
}

em, i {
  font-style: italic;
}

ul {
  list-style: outside disc;
}

ol {
  list-style: outside decimal;
}

html, body {
  overflow-x: hidden;
}

body {
  width: 100%;
  margin: 0;
  padding: 0;
  color: #5d686f;
  font-size: $bodysize;
  line-height: $bodysize * 1.5;
  letter-spacing: .01em;
  text-align: left;
}

section {
  padding: $padding;
}

a {
  color: #226ae9;
  text-decoration: none;
  &:hover {
    color: #349ef3;
  }
}

a.tag {
  display: inline-block;
  background: #fff;
  width: auto;
  height: auto;
  border: 1px solid #349ef3;
  border-radius: 3px;
  letter-spacing: .01em;
  cursor: pointer;
  margin: 0 4px 7px 0;
  padding: 2px 10px;
}

section, article, aside, footer, header, nav {
  display:block;
}

#blog, #cover {
  top: 0;
  left: 0;
}

#blog {
  position: absolute;
  width: 100%;
  height: 100%;
}

#blog.pushed {
  overflow-x: hidden;
}

#header {
  position: fixed;
  width: 100%;
  height: 50px;
  top: 0;
  border: 1px solid #eef2f8;
  background-color: #fff;
  color: #88909a;
  z-index: 10;
  .header-title {
    text-align: center;
    font-family: $font_serif;
    font-size: 1.2rem;
    line-height: 50px;
    margin: 0;
  }
  .header-title-link {
    color: #88909a;
  }
  .header-right {
    position: absolute;
    top: 12px;
    right: 20px;
    color: #88909a;
  }
  .header-picture {
    display: block;
    width: 30px;
    height: 30px;
    border-radius: 15px;
  }
}

#header.pushed {
  -webkit-transform: translate3d($sidebarwidth, 0, 0);
  -moz-transform: translate3d($sidebarwidth, 0, 0);
  transform: translate3d($sidebarwidth, 0, 0);
}

#sidebar {
  font-family: $font_sans;
  position: fixed;
  width: $sidebarwidth;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 20;
  overflow: auto;    
  display: -webkit-flex;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  flex-direction: column;
  justify-content: center;
  -webkit-transition: -webkit-transform .25s ease-in-out;
  transition: transform .25s ease-in-out;
  a {
    color: #ebebeb;
    &:hover {
      color: #fff;
      text-decoration: underline;
    }
  }
  ul {
    display: inline-block;
    width: 79%;
    vertical-align: top;
  }
  li {
    display: block;
    width: 100%;
    height: 30px;
  }
  .sidebar-container {
    overflow: auto;
    position: relative;
    padding: 0 40px;
  }
  .sidebar-profile {
    color: #ebebeb;
    text-align: center;
    padding-top: 18px;
    margin-bottom: 15px;
  }
  .sidebar-profile-picture {
    width: 90px;
    height: 90px;
    border-radius: 90px;
    text-align: center;
    margin: 0 auto;
  }
  .sidebar-profile-name {
    text-align: center;
  }
}

#sidebar.pushed {
  -webkit-transform: translate3d($sidebarwidth, 0, 0);
  -moz-transform: translate3d($sidebarwidth, 0, 0);
  transform: translate3d($sidebarwidth, 0, 0);
}

#main {
  background: #fff;
  width: 100%;
  min-height: 100%;
}

#main.pushed {
  -webkit-transform: translate3d($sidebarwidth, 0, 0);
  -moz-transform: translate3d($sidebarwidth, 0, 0);
  transform: translate3d($sidebarwidth, 0, 0);
}

#footer {
  color: #9eabb3;
  text-align: center;
  font-size: 0.8em;
  padding: $padding;
}

#cover {
  position: fixed;
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-size: cover;
  z-index: -1;
}

#button-open-sidebar {
  position: absolute;
  top: 12px;
  left: 20px;
  cursor: pointer;
  color: #666;
}

#button-close-sidebar {
  height: 100%;
  width: 0;
  position: fixed;
  z-index: 20;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0, 0.9);
  overflow-y: auto;
  overflow-x: hidden;
  text-align: center;
  opacity: 0;
  transition: opacity 1s;
}

#button-close-sidebar.pushed {
  width: 100%;
  opacity: 0.8;
  -webkit-transform: translate3d($sidebarwidth, 0, 0);
  -moz-transform: translate3d($sidebarwidth, 0, 0);
  transform: translate3d($sidebarwidth, 0, 0)
}

// responsive main and sidebar
@media only screen and (max-width: 1023px) { 
  #sidebar {
    left: 0 - $sidebarwidth;
  }
  #header {
    display: auto;
  }
  #main {
    width: 100%;
    padding-top: 60px;
  }
}

@media only screen and (min-width: 1024px) {
  // show sidebar, hide header
  #sidebar[data-behavior='1'] {
    left: 0;
  }
  #header[data-behavior='1'] {
    display: none;
  }
  #main[data-behavior='1'] {
    width: calc(100% - #{$sidebarwidth});
    float: right;
  }
  // hide sidebar, show header
  #sidebar[data-behavior='2'] {
    left: 0 - $sidebarwidth;
  }
  #header[data-behavior='2'] {
    display: auto;
  }
  #main[data-behavior='2'] {
    width: 100%;
    padding-top: 60px;
  }
}

.content-wrap {
  display: block;
  max-width: 1200px;
  margin: 0 auto;
  padding-right: 10px;
  padding-left: 10px;
}

.post-list {
  list-style: none;
  margin: 0;
  padding: $padding;
  li {
    border-bottom: 1px solid #ccc;
  }
}

.post-info {
  font-family: $font_sans;
  font-size: 0.8em;
  text-align: right;
  margin-bottom: 1em;
  .label {
    background-color: lightblue;
    text-transform: uppercase;
    letter-spacing: .01em;
    padding: 0px 4px;
    border-radius: 3px;
  }
}

.post-title {
  a, a:visited {
    color: #5d686f;
    &:hover {
      color: #999;
    }
  }
}

.post-content {
  font-family: $font_serif;
  p {
    margin-bottom: 1em;
  }
  strong {
    background-color: LightYellow;
    border-radius: 5px;
  }
  iframe {
    margin-bottom: 1em;
  }
  img {
    box-shadow: 3px 3px 6px #dddddd;
    margin: 0 auto 1.5em auto;
    max-width: 100%;
    display: block!important;
  }
  .img-center {
    margin: 0.8em auto;
    padding: 0;
    text-align: center;
    clear: both;
  }
  .img-left {
    margin: 0.8em auto;
    padding: 0;
    clear: both;
  }
  .line-block {
    margin-bottom: 1em;
  }
}

img.right, figure.right {
  float: right;
  margin: 0 0 2em 2em;
}

img.left, figure.left {
  float: left;
  margin: 0 2em 2em 0;
}

.readmore {
  padding: 0 0.5em;
  font-size: 1.2em;
}

// responsive blogger-ads and iframe
@media screen and (max-device-width: 768px) {
  #blogger-ads {
    display: none;
  }
}

@media only screen and (max-width: 1023px) {
  #blogger-ads {
    display: none;
  }
  .youtube {
    position: relative;
    padding-bottom: $youtubesize;
    padding-top: 25px;
    height: 0;
    overflow: hidden;
  }
  .youtube iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    margin-bottom: 1em;
  }
}

#google-custom-search {
  width: 300px;
  margin-top: 6px;
  padding: 0 15px 0 10px;
  .gsc-control-cse {
    padding: 0 !important;
    border: 0 !important;
    background-color: transparent !important;
  }
  .gsc-input {
    margin: -6px 0 0 0 !important;
    padding: 2px 4px 0 0 !important;
    border: none !important;
    box-shadow: none !important;
  }
  .gsc-input-box {
  }
  .gsc-search-button {
    box-sizing: content-box !important;
    box-shadow: none !important;
  }
}

#comments-list {
  list-style: none;
  margin: 0 1em;
  blockquote {
    background: #f8f8f8;
    clear: both;
    font-style: normal;
    margin: 0;
    padding: 15px 20px;
    border-radius: 5px;
  }
  footer {
    color: #888;
    padding: 0.5em 1em 0 0;
    text-align: right;
  }
  li:nth-child(2n) blockquote {
    background: #F5f5f5;
  }
}

#add-comment {
  label {
    clear: left;
    float: left;
    text-align: left;
  }
  input {
    &[type='text'], &[type='email'], &[type='url'] {
      float: left;
    }
  }
  textarea {
    float: left;
    height: 150px;
  }
  p.req {
    clear: both;
    margin: 0 0.5em 1em 0;
    text-align: right;
  }
  input[type='submit'] {
    float: right;
    margin: 0 0.5em;
  }
  * {
    margin-bottom: .5em;
  }
}

// overwriting pygment.css
.err {
  border: 0;
}

// blogger ads
#blogger-ads {
  text-align: center;
}

#blogger-ads li {
  list-style-type: none;
}

/* reStructuredText */
.strike {
  text-decoration: line-through;
}

/* Magnific Popup CSS */
/* remove shadow and bg-color */
.mfp-figure:after {
  box-shadow: none !important;
  background: none !important;
}

/* Gallery CSS */
table.gallery {
  margin: 0 auto;
  border: 0;
  text-align: center;
  font-size: 1.0em;
  font-weight: bold;
  th, td {
    padding: 0.5em;
    border-top: 0;
    border-left: 0;
    border-right: 0;
    border-bottom: 1px solid #ccc;
    border-spacing: 5px 10px;
    vertical-align: middle;
  }
  caption {
    margin: 1.5em 0 0.2em 0;
    padding: 0.2em;
    line-height: 2em;
    font-size: 1.2em;
    font-weight: bold;
    text-align: center;
  }
  /* Responsive images in tables */
  img {
    display: inline-block;
    width: auto;
    max-width: 100%;
    height: auto;
    max-height: 100%;
  }
}

.breadcrumb {
  padding: 8px 15px;
  margin-bottom: 20px;
  list-style: none;
  background-color: #f5f5f5;
  border-radius: 4px; 
  .active {
    color: #777;
  }
}
.breadcrumb>li+li:before {
  padding: 0 5px;
  color: #ccc;
  content: "/\00a0";
}
.breadcrumb>li {
  display: inline-block;
}

.archive_row {
  font-family: "Microsoft YaHei", Overlock, sans-serif;
  margin-right: -15px;
  margin-left: -15px;
  .archive_year{
    color: #113D8B;
    font-size: $h2size;
    font-weight: bold;
    margin-top: $h2size;
    padding-left: 20px;
  }
  .archive_month{
    color: #000;
    font-size: $h3size;
    font-weight: bold;
    margin-top: $h3size;
    padding-left: 40px;
  }
  .archive_day {
    padding-left: 80px;
    padding-right: 20px;
    width: 20px;
    float: left;
  }
}

.paginator {
  text-align: center;
}
.pagination {
  display: inline-block;
  margin: 20px 0;
  border-radius: 4px;
}
.pagination>li {
  display: inline;
}
.pagination>li>a,
.pagination>li>span {
  position: relative;
  float: left;
  padding: 6px 12px;
  margin-left: -1px;
  line-height: 1.42857143;
  color: #337ab7;
  text-decoration: none;
  background-color: #fff;
  border: 1px solid #ddd;
}
.pagination>li:first-child>a,
.pagination>li:first-child>span {
  margin-left: 0;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
}
.pagination>li:last-child>a,
.pagination>li:last-child>span {
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
.pagination>li>a:focus,
.pagination>li>a:hover,
.pagination>li>span:focus,
.pagination>li>span:hover {
  z-index: 2;
  color: #23527c;
  background-color: #eee;
  border-color: #ddd;
}
.pagination>.active>a,
.pagination>.active>a:focus,
.pagination>.active>a:hover,
.pagination>.active>span,
.pagination>.active>span:focus,
.pagination>.active>span:hover {
  z-index: 3;
  color: #fff;
  cursor: default;
  background-color: #337ab7;
  border-color: #337ab7;
}
.pagination>.disabled>a,
.pagination>.disabled>a:focus,
.pagination>.disabled>a:hover,
.pagination>.disabled>span,
.pagination>.disabled>span:focus,
.pagination>.disabled>span:hover {
  color: #777;
  cursor: not-allowed;
  background-color: #fff;
  border-color: #ddd;
